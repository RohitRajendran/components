    "sass-lint-auto-fix": "0.21.0",
    "sass-loader": "8.0.2",
    "sinon": "8.0.4",
    "style-loader": "1.1.2",
    "tap-xunit": "2.4.1",
    "tape": "4.13.0",
    "text-mask-addons": "3.8.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "xstate": "4.7.6"
  },
  "peerDependencies": {
    "accounting": "0.4.1",
    "classnames": "2.2.6",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "query-string": "<6.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-spring": "8.0.27",
    "react-text-mask": "5.4.3",
    "react-tooltip": "3.11.2",
    "xstate": "4.7.6"
  }
}
