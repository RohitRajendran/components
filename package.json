{
  "name": "@unitedincome/components",
  "description": "Component library for all United Income applications.",
  "author": "United Income",
  "version": "1.0.93",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook",
    "build": "webpack",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280 -s storybook-static",
    "test": "yarn testSingle './components/**/*.spec.js' './proptypes/**/*.spec.js' | faucet",
    "testCI": "NODE_ENV=test nyc tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles './components/**/*.spec.js' './proptypes/**/*.spec.js'  | tap-xunit > ./reports/tape/results.xml",
    "coverage": "nyc report",
    "testSingle": "NODE_ENV=test tape -r @babel/register -r ./utils-for-tests/configureEnzyme.js -r jsdom-global/register -r ignore-styles",
    "buildStorybook": "build-storybook -c .storybook -o .build",
    "lint": "eslint './components/**/*.js' './proptypes/**/*.js' './.storybook/*.js' --cache && sass-lint -v -q && prettier -l './components/**/*.js' './proptypes/**/*.js' && prettier -l './components/**/*.md' './*.md' --parser=markdown && prettier -l './components/**/*.mdx' './*.mdx' --parser=mdx && prettier -l './components/**/*.scss' './constants/sass/**/*.scss' './constants/sass/*.scss' --parser=scss || (echo need formatting, use yarn format ; exit 1)",
    "format": "prettier --write './components/**/*.js' './proptypes/**/*.js' './.storybook/*.js' && prettier --write  './components/**/*.md' './*.md' --parser=markdown && prettier --write './components/**/*.mdx' './*.mdx' --parser=mdx && sass-lint-auto-fix && prettier --write './components/**/*.scss' './constants/sass/**/*.scss' './constants/sass/*.scss' --parser=scss",
    "generate": "yarn hygen component new",
    "mcgonagall": "yarn hygen mcgonagall new",
    "browserstack": "yarn nightwatch -c nightwatch/config.js",
    "testVisual": "node ./utils-for-tests/branch.js && yarn snapshot || (echo 'Visual tests not required!')",
    "dsm-storybook:publish": "dsm-storybook publish",
    "dsm-storybook:preview": "dsm-storybook preview"
  },
  "homepage": "https://github.com/UnitedIncome/components",
  "repository": {
    "type": "git",
    "url": "https://github.com/UnitedIncome/components.git"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/plugin-transform-spread": "7.8.3",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.8.3",
    "@babel/register": "7.8.3",
    "@babel/runtime": "7.8.3",
    "@percy/storybook": "3.2.0",
    "@sambego/storybook-state": "1.3.6",
    "@storybook/addon-a11y": "5.3.8",
    "@storybook/addon-options": "^5.3.8",
    "@storybook/addon-backgrounds": "5.3.3",
    "@storybook/addon-docs": "5.2.8",
    "@storybook/addon-knobs": "5.3.8",
    "@storybook/addon-viewport": "5.3.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "5.2.8",
    "@invisionapp/dsm-storybook": "^0.0.123",
    "accounting": "0.4.1",
    "airbnb-prop-types": "2.15.0",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-module-resolver": "4.0.0",
    "bootstrap": "4.4.1",
    "classnames": "2.2.6",
    "core-js": "3.6.4",
    "css-loader": "3.4.2",
    "d3-array": "=1.2.4",
    "d3-interpolate": "1.4.0",
    "d3-scale": "=2.2.2",
    "d3-shape": "1.3.7",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "faucet": "0.0.1",
    "file-loader": "5.0.2",
    "hygen": "5.0.3",
    "ignore-styles": "5.0.1",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "moment": "2.24.0",
    "nightwatch": "1.3.2",
    "node-sass": "4.13.0",
    "nyc": "15.0.0",
    "postcss-loader": "3.0.0",
    "postcss-prefixer": "2.1.2",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "query-string": "=5.1.1",
    "rc-slider": "8.7.1",
    "react": "16.12.0",
    "react-beautiful-dnd": "10.1.1",
